<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleIndicator">
        <attr name="ci_height" format="dimension"/>
        <attr name="ci_width" format="dimension"/>
        <attr name="ci_margin" format="dimension"/>
        <attr name="ci_drawable" format="reference"/>
        <attr name="ci_drawable_unselected" format="reference"/>
    </declare-styleable>

    <!-- LeafLoading -->
    <declare-styleable name="IQiYiLoading">
        <attr name="triangleColor" format="color" />
        <attr name="circleColor" format="color" />
        <attr name="duration" format="integer" />
        <attr name="circleWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="LeafLoadingView">
        <attr name="maxProgress" format="integer"/>
        <attr name="leafRotateTime" format="integer"/>
        <attr name="leafFlyTime" format="integer"/>
        <attr name="amplitudeDisparity" format="integer"/>
    </declare-styleable>
</resources>